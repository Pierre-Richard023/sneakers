{% extends 'base.html.twig' %}

{% block title %}Sneakers Pluss{% endblock %}

{% block body %}

    <section class="main-brand-selected">

        {% for brand in brands %}
            <a href="{{ path('collections.brands',{slug : brand.slug}) }}">
                <div class="brand-card">
                    <h2>{{ brand.name }}</h2>
                    <img src=" {{ vich_uploader_asset(brand) }}"
                         alt="{{ brand.name }} logo"/>
                </div>
            </a>
        {% endfor %}

    </section>


    <section class="main-collection-news">

        {% for sneaker in sneakers %}
            <a href="{{ path('sneaker.show',{ slug : sneaker.slug }) }}">
                <div class="product">
                    <img alt="sneaker"
                         src="data:image/jpeg;base64,">
                    <div class="product-info">
                        <h4 class="product-title"> {{ sneaker.name }}</h4>
                        <p class="product-price">$129</p>
                    </div>
                </div>
            </a>
        {% endfor %}

    </section>

{% endblock %}
